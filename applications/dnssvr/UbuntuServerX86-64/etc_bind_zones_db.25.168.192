;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.nwsecdocker.jhu.edu. root.nwsecdocker.jhu.edu. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
	IN	NS	ns.nwsecdocker.jhu.edu.

