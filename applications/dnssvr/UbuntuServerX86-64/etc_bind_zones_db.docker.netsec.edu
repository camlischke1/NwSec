;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.docker.netsec.edu. root.docker.netsec.edu (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
	IN	NS	ns.docker.netsec.edu.

; name servers - A records
ns.docker.netsec.edu.	IN	A	192.168.25.10

; hosts
suricata.docker.netsec.edu.	IN	A	192.168.25.100
auth.docker.netsec.edu.		IN	A	192.168.25.101
proxy.docker.netsec.edu.	IN	A	192.168.25.102
selks.docker.netsec.edu.	IN	A	192.168.25.103
nginx.docker.netsec.edu.	IN	A	192.168.25.104
mail.docker.netsec.edu.		IN	A	192.168.25.105
nfs.docker.netsec.edu.		IN	A	192.168.25.106
telnet.docker.netsec.edu.	IN	A	192.168.25.107
ftp.docker.netsec.edu.		IN	A	192.168.25.108
